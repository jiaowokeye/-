AFzC,CATF,EAaE,oBAAC,IAAD;AAAM,IAAA,EAAE,EAAC,OAAT;AAAiB,IAAA,SAAS,EAAE9B,GAAG,CAAC,WAAD;AAA/B,gCAbF,EAgBE,oBAAC,IAAD;AAAM,IAAA,EAAE,EAAC,aAAT;AAAuB,IAAA,SAAS,EAAEA,GAAG,CAAC,WAAD;AAArC,4CAhBF,EAmBE,oBAAC,IAAD;AAAM,IAAA,EAAE,EAAC,UAAT;AAAoB,IAAA,SAAS,EAAEA,GAAG,CAAC,WAAD;AAAlC,4CAnBF,EAsBE;AAAM,IAAA,SAAS,EAAEA,GAAG,CAAC,WAAD,CAApB;AAAmC,IAAA,OAAO,EAAEY;AAA5C,kDAtBF,CADF;AA0BD","sourcesContent":["import React, {\n  useEffect,\n  useReducer,\n  useContext \n} from 'react';\nimport {\n  ExceptionTip\n} from '@wecode/react-weui';\nimport i18n from 'i18n';\nimport { Link } from 'react-router-dom';\nimport * as css from './index.module.css';\nimport { GLOBAL_EXAMPLE_DATA } from '../../actions/global';\nimport { fetchDataDemoAction, FETCH_DEMO_INIT, FETCH_DEMO_SUCCESS, FETCH_DEMO_FAILURE } from '../../actions/home';\nimport homeReducer, { initialState } from '../../reducers/home';\nimport AppContext from '../../AppContext';\n\nexport default function Home() {\n  // 示例代码，可删除：获取全局的status，路由组件和其他组件可通过此方法获取到。\n  const [appState, appDispatch] = useContext(AppContext);\n  // 示例代码，可删除：执行全局的action，可改变全局的state。\n  function handleClick() {\n    appDispatch({ type: GLOBAL_EXAMPLE_DATA, payload: '改变全局status示例' });\n  }\n  console.log(appState, '--- appState');\n\n  const [state, dispatch] = useReducer(homeReducer, initialState);\n\n  console.log(state, '--- home state');\n  useEffect(() => {\n    // 设置导航标题\n    HWH5.navTitle({ title: 'Hello World' });\n    // prevent setting state for unmounted components.\n    let ignore = false;\n    dispatch({ type: FETCH_DEMO_INIT });\n    console.log('fetch:start.')\n    HWH5.showLoading();\n    fetchDataDemoAction().then((payload)=> {\n      HWH5.hideLoading();\n      if (!ignore) {\n        dispatch({ type: FETCH_DEMO_SUCCESS, payload });\n        console.log('fetch:done.');\n      }\n    }).catch((error)=> {\n      HWH5.hideLoading();\n      dispatch({ type: FETCH_DEMO_FAILURE });\n      console.log('Error:', error);\n    });\n    return () => {\n      // 销毁事件\n      ignore = true;\n    }\n  }, []);\n\n  const { isError } = state;\n\n  return (\n    <div className={css.App}>\n      {isError &&\n        <ExceptionTip\n          flag={1}\n          msg={'请求数据异常！'}\n          submsg={'请重试'}\n        /> }\n      <div className={css['App-header']} />\n      <h1 className={css['App-title']}>{i18n.t('common:welcome')}</h1>\n      <p className={css['App-intro']}>\n        {i18n.t('home:getStart')} {i18n.t('home:edit')}\n        <code>src/routes/Home/index.js</code> {i18n.t('home:saveReload')}\n      </p>\n      <Link to='/desc' className={css['home-link']}>\n        使用说明\n        </Link>\n      <Link to='/navigateto' className={css['home-link']}>\n        页面跳转示例\n        </Link>\n      <Link to='/refresh' className={css['home-link']}>\n        下拉刷新示例\n      </Link>\n      <span className={css['home-link']} onClick={handleClick}>改变全局status示例</span>\n    </div>\n  );\n}\n"]}]}