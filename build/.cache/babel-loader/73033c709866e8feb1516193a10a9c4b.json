A,QAAQ,EAAE;AADD,SAAb;AAGA;;AACJ,WAAK,GAAL;AACIJ,QAAAA,OAAO,CAACG,IAAR,CAAa;AACTC,UAAAA,QAAQ,EAAE;AADD,SAAb;AAGA;;AACJ;AACI;AAvBR;AAyBH;;AACD,MAAMC,MAAM,GACZ,oBAAC,IAAD;AACI,IAAA,SAAS,EAAExD,GAAG,CAACyD,IAAJ,GAAS,kBADxB;AAEI,IAAA,IAAI,EAAEjD,IAFV;AAGI,IAAA,KAAK,EAAE,CAHX;AAII,IAAA,QAAQ,EAAE,kBAACwC,IAAD;AAAA,aAAQD,SAAQ,CAACC,IAAD,CAAhB;AAAA,KAJd;AAKI,IAAA,MAAM,EAAEU,QAAQ,CAACC,eAAT,CAAyBC;AALrC,IADA;;AASA,WAASC,OAAT,GAAkB;AAAA,QACNV,OADM,GACMvC,KADN,CACNuC,OADM;AAEdA,IAAAA,OAAO,CAACG,IAAR,CAAa;AACTC,MAAAA,QAAQ,EAAE;AADD,KAAb;AAGH;;AACD,SAAO,iCACF,oBAAC,MAAD;AACG,IAAA,IAAI,EAAC,OADR;AAEG,IAAA,IAAI,EAAE;AAAG,MAAA,OAAO,EAAE;AAAA,eAAIM,OAAO,EAAX;AAAA,OAAZ;AAA2B,MAAA,KAAK,EAAC;AAAjC,MAFT;AAGG,IAAA,WAAW,EAAE;AAAA,aAAKlC,SAAS,EAAd;AAAA,KAHhB;AAIG,IAAA,YAAY,EAAE,CACV;AAAG,MAAA,KAAK,EAAC,6BAAT;AAAuC,MAAA,OAAO,EAAE;AAAA,eAAImB,UAAU,EAAd;AAAA;AAAhD,MADU;AAJjB,KAOA;AAAM,IAAA,SAAS,EAAE9C,GAAG,CAACe;AAArB,IAPA,EAOoCA,KAPpC,CADE,EASDF,IAAI,GAAI2C,MAAJ,GAAa,iCAThB,EAWE/B,IAAI,CAACqC,GAAL,CAAS,UAACC,CAAD,EAAGC,CAAH,EAAO;AACZ,WAAO,oBAAC,IAAD;AAAM,MAAA,GAAG,EAAEA,CAAX;AAAc,MAAA,YAAY,EAAE;AAAA,eAAM,iCACpCD,CAAC,CAACE,SADkC,CAAN;AAAA,OAA5B;AAEC,MAAA,SAAS,EAAC;AAFX,OAGP,oBAAC,IAAD,QAEQF,CAAC,CAACG,MAAF,CAASC,OAAT,CAAiBC,KAAjB,GAAuB;AAAK,MAAA,SAAS,EAAEpE,GAAG,CAACqE,WAApB;AAAiC,MAAA,GAAG,EAAEN,CAAC,CAACG,MAAF,CAASC,OAAT,CAAiBC;AAAvD,MAAvB,GAAuF;AAAG,MAAA,KAAK,EAAC;AAAT,MAF/F,EAKQL,CAAC,CAACG,MAAF,CAASrB,IALjB,EAQQkB,CAAC,CAACG,MAAF,CAASnD,KARjB,CAHO,EAcP,oBAAC,IAAD;AAAM,MAAA,KAAK,EAAC,OAAZ;AAAoB,MAAA,SAAS,EAAC,KAA9B;AAAoC,MAAA,IAAI,MAAxC;AAAyC,MAAA,KAAK,EAC3C,iCACKgD,CAAC,CAACO,WAAF,GAAc,EAAd,GAAiB,iDADtB;AADH,OAMOP,CAAC,CAACQ,aANT,CAdO,CAAP;AAwBH,GAzBD,CAXF,EAuCE9C,IAAI,CAAC+C,MAAL,KAAc,CAAd,GAAgB;AAAK,IAAA,KAAK,EAAE;AAACC,MAAAA,SAAS,EAAC,QAAX;AAAoBC,MAAAA,UAAU,EAAC;AAA/B;AAAZ,4CAAhB,GAAkF,gCAvCpF,CAAP;AA0CH;;AAED,eAAe/D,IAAf","sourcesContent":["import React,{useState,useEffect} from 'react';\r\nimport * as css from './index.module.css';\r\nimport { Menu, NavBar,List } from 'antd-mobile';\r\nimport moment from 'moment';\r\nimport {getPlanList} from './../../servers/comp'\r\nconst {Item} = List;\r\nlet time1 = null;\r\nconst data = [\r\n    {\r\n        value: '1',\r\n        label: '默认',\r\n    }, {\r\n        value: '2',\r\n        label: '指定日期',\r\n    },\r\n    {\r\n        value: '3',\r\n        label: '指定部门',\r\n    },\r\n    {\r\n        value: '4',\r\n        label: '指定个人',\r\n    },\r\n];\r\nfunction Plan(props){\r\n    const [show,setShow] = useState(false);\r\n    const [title,setTitle] = useState('计划任务');\r\n    const [date,setDate] = useState();\r\n    const [menuType,setMenuType] = useState('1');\r\n    const [chooseUser,setChooseUser] = useState(null);\r\n    const [chooseGroup,setChooseGroup] = useState(null);\r\n    const [list,setList] = useState([]);\r\n    function LeftClick(){\r\n        // setShow(true);\r\n    }\r\n    function getProgressListData(){\r\n        getPlanList({\r\n            pageNo: 1,\r\n            pageSize: 100,\r\n            user_id\t: chooseUser?chooseUser.user_id:-1,\r\n            group_id: chooseGroup?chooseGroup.groupId:0,\r\n            type: 0,\r\n            startdate: date,\r\n            enddate: date,\r\n        }).then((res)=>{\r\n            setList(res.data.paginationData?res.data.paginationData:[]);\r\n        });\r\n    }\r\n    useEffect(()=>{\r\n        if(time1){\r\n            clearTimeout(time1);\r\n        }\r\n        time1 = setTimeout(()=>{\r\n            getProgressListData();\r\n        },1000);\r\n    },[date,menuType,chooseUser,chooseGroup])\r\n    useEffect(()=>{\r\n        const menuType = window.menuType?window.menuType:'1';\r\n        setMenuType(menuType);\r\n        switch(menuType){\r\n            case \"1\":\r\n                setTitle('计划任务');\r\n                setChooseUser(null);\r\n                setChooseGroup(null);\r\n                setDate('');\r\n                break;\r\n            case \"2\":\r\n                if(window.chooseDate){\r\n                    setDate(window.chooseDate);\r\n                    setTitle(window.chooseDate);\r\n                    setChooseUser(null);\r\n                    setChooseGroup(null);\r\n                }\r\n                break;\r\n            case \"3\":\r\n                if(window.chooseGroup){\r\n                    setDate('');\r\n                    setTitle(window.chooseGroup.groupname);\r\n                    setChooseUser(null);\r\n                    setChooseGroup(window.chooseGroup);\r\n                }\r\n                break;\r\n            case \"4\":\r\n                if(window.chooseUser){\r\n                    setDate('');\r\n                    setTitle(window.chooseUser.name);\r\n                    setChooseUser(window.chooseUser);\r\n                    setChooseGroup(null);\r\n                }\r\n                break;\r\n            default:\r\n                break;\r\n        }\r\n        \r\n    },[1])\r\n    function RightClick(){\r\n        setShow(true);\r\n    }\r\n    function onChange(item){\r\n        console.log(item);\r\n        const value = item[0];\r\n        const { history } = props;\r\n        window.tab = 2;\r\n        window.chooseDate = date;\r\n        window.menuType = value;\r\n        switch(value){\r\n            case \"1\":\r\n                setTitle('计划任务');\r\n                setShow(false);\r\n                setDate(moment().format('YYYY-MM-DD'));\r\n                break;\r\n            case \"2\":\r\n                //跳转内部页面并传值\r\n                history.push({\r\n                  pathname: '/chooseDate/',\r\n                })\r\n                break;\r\n            case \"3\":\r\n                history.push({\r\n                    pathname: '/chooseGroup/',\r\n                  })\r\n                break;\r\n            case \"4\":\r\n                history.push({\r\n                    pathname: '/chooseUser/',\r\n                })\r\n                break;\r\n            default:\r\n                break;\r\n        }\r\n    }\r\n    const menuEl = (\r\n    <Menu\r\n        className={css.menu+' single-foo-menu'}\r\n        data={data}\r\n        level={1}\r\n        onChange={(item)=>onChange(item)}\r\n        height={document.documentElement.clientHeight}\r\n    />\r\n    );\r\n    function AddPlan(){\r\n        const { history } = props;\r\n        history.push({\r\n            pathname: '/addPlan/',\r\n        })\r\n    }\r\n    return <div>\r\n         <NavBar\r\n            mode=\"light\"\r\n            icon={<i onClick={()=>AddPlan()} class=\"icon-nav icon-nav-addPress\"></i>}\r\n            onLeftClick={() =>LeftClick() }\r\n            rightContent={[\r\n                <i class=\"icon-nav icon-nav-morePress\" onClick={()=>RightClick()}></i>\r\n            ]}\r\n        ><span className={css.title}></span>{title}</NavBar>\r\n         {show  ? menuEl : <span/>}\r\n         {\r\n             list.map((e,i)=>{\r\n                 return <List key={i} renderHeader={() => <div>\r\n                     {e.plan_date}\r\n                 </div>} className=\"my-list\">\r\n                 <Item>\r\n                     {\r\n                         e.doUser.extInfo.photo?<img className={css.headerPhoto} src={e.doUser.extInfo.photo}/>:<i class=\"icon-nav icon-nav-headPortrait\"></i>\r\n                     }\r\n                     {\r\n                         e.doUser.name\r\n                     }\r\n                     {\r\n                         e.doUser.title\r\n                     }\r\n                 </Item>\r\n                 <Item arrow=\"empty\" className=\"spe\" wrap extra={\r\n                    <div>\r\n                        {e.plan_status?'':<span >开始</span>}\r\n                    </div>\r\n                }>\r\n                    {\r\n                        e.customer_name\r\n                    }\r\n                 </Item>\r\n               </List>\r\n             })\r\n         }\r\n         {\r\n             list.length===0?<div style={{textAlign:'center',paddingTop:'100px'}}>暂无计划信息</div>:<div></div>\r\n         }\r\n    </div>\r\n}\r\n\r\nexport default Plan;"]}]}